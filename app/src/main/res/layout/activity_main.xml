<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/Scann"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/titulo"
        android:layout_marginTop="130dp"
        android:text="Scan Qrcode"
        android:textColor="#000000"
        android:textSize="25sp"
        app:backgroundTint="#FDFDFD" />

    <Button
        android:id="@+id/id_maquina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/Scann"
        android:layout_marginTop="130dp"
        android:text="ID manual"
        android:textColor="#000000"
        android:textSize="25sp"
        app:backgroundTint="#FDFDFD" />

    <Button
        android:id="@+id/videochamada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/id_maquina"
        android:layout_marginTop="130dp"
        android:text="Videochamada"
        android:textColor="#000000"
        android:textSize="25sp"
        app:backgroundTint="#FDFDFD" />

    <TextView
        android:id="@+id/titulo"
        android:text="Manutenção"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="50sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center_horizontal" />
</RelativeLayout>
